    "code": "object Solution {\n    def minLength(s: String): Int = {\n        // Use a StringBuilder as a stack for efficient append and remove operations\n        val stack = new StringBuilder\n\n        s.foreach { c =>\n            if (stack.nonEmpty && ((stack.last == 'A' && c == 'B') || (stack.last == 'C' && c == 'D'))) {\n                // Remove the last character if it forms \"AB\" or \"CD\" with the current character\n                stack.deleteCharAt(stack.length - 1)\n            } else {\n                // Append the current character if it does not form \"AB\" or \"CD\" with the last character of the stack\n                stack.append(c)\n            }\n        }\n\n        // The length of the stack (StringBuilder) is the minimum length of the string we can obtain\n        stack.length\n    }\n}"
