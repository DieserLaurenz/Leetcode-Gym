    "code": "impl Solution {\n    pub fn make_smallest_palindrome(s: String) -> String {\n        let mut chars: Vec<char> = s.chars().collect();\n        let mut i = 0;\n        let mut j = s.len() - 1;\n        \n        while i < j {\n            if chars[i] != chars[j] {\n                // Replace the character on the right with the one on the left\n                // to ensure lexicographically smallest palindrome\n                if chars[i] > chars[j] {\n                    chars[i] = chars[j];\n                } else {\n                    chars[j] = chars[i];\n                }\n            }\n            i += 1;\n            j -= 1;\n        }\n        \n        chars.into_iter().collect()\n    }\n}"
