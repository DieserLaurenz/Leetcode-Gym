    "code": "class Solution {\n    fun makeSmallestPalindrome(s: String): String {\n        val chars = s.toCharArray()\n        val n = s.length\n        for (i in 0 until n / 2) {\n            if (chars[i] != chars[n - i - 1]) {\n                if (chars[i] > chars[n - i - 1]) {\n                    chars[i] = chars[n - i - 1]\n                } else {\n                    chars[n - i - 1] = chars[i]\n                }\n            }\n        }\n        return String(chars)\n    }\n}"
