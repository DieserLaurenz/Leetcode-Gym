    "code": "-spec make_smallest_palindrome(S :: unicode:unicode_binary()) -> unicode:unicode_binary().\nmake_smallest_palindrome(S) ->\n    List = binary_to_list(S),\n    PalindromeList = make_palindrome(List, length(List)),\n    list_to_binary(PalindromeList).\n\nmake_palindrome(List, Len) ->\n    make_palindrome_helper(List, 0, Len - 1).\n\nmake_palindrome_helper(List, Left, Right) when Left >= Right -> List;\nmake_palindrome_helper(List, Left, Right) ->\n    LeftChar = lists:nth(Left + 1, List),\n    RightChar = lists:nth(Right + 1, List),\n    if\n        LeftChar =:= RightChar ->\n            make_palindrome_helper(List, Left + 1, Right - 1);\n        true ->\n            SmallerChar = min(LeftChar, RightChar),\n            UpdatedList = lists:sublist(List, Left) ++\n                          [SmallerChar] ++\n                          lists:nthtail(Left + 1, lists:sublist(List, Right - Left)) ++\n                          [SmallerChar] ++\n                          lists:nthtail(Right + 1, List),\n            make_palindrome_helper(UpdatedList, Left + 1, Right - 1)\n    end."
