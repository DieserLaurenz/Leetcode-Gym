    "code": "function makeSmallestPalindrome(s: string): string {\n    let arr = s.split('');\n    let left = 0;\n    let right = s.length - 1;\n\n    while (left < right) {\n        // If characters at left and right are not the same,\n        // update the character on the right to the smaller one\n        // to ensure lexicographically smallest palindrome.\n        if (arr[left] !== arr[right]) {\n            if (arr[left] > arr[right]) {\n                arr[left] = arr[right];\n            } else {\n                arr[right] = arr[left];\n            }\n        }\n        left++;\n        right--;\n    }\n\n    return arr.join('');\n}"
