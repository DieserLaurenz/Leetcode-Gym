    "code": "class Solution {\n    func canBeEqual(_ s1: String, _ s2: String) -> Bool {\n        // Convert strings to arrays for easier character access\n        let arr1 = Array(s1)\n        let arr2 = Array(s2)\n\n        // Check if characters at even and odd indices can match with swaps\n        let evenMatches = (arr1[0] == arr2[0] || arr1[0] == arr2[2]) && (arr1[2] == arr2[0] || arr1[2] == arr2[2])\n        let oddMatches = (arr1[1] == arr2[1] || arr1[1] == arr2[3]) && (arr1[3] == arr2[1] || arr1[3] == arr2[3])\n\n        return evenMatches && oddMatches\n    }\n}"
