    "code": "class Solution {\n    fun maximumNumberOfStringPairs(words: Array<String>): Int {\n        var pairs = 0\n        val wordMap = mutableMapOf<String, Int>()\n\n        words.forEach { word ->\n            val reversed = word.reversed()\n            if (wordMap.containsKey(reversed)) {\n                pairs++\n                wordMap.remove(reversed)\n            } else {\n                wordMap[word] = wordMap.getOrDefault(word, 0) + 1\n            }\n        }\n\n        return pairs\n    }\n}"
