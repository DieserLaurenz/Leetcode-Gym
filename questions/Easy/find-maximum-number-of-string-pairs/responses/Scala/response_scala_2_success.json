    "code": "object Solution {\n    def maximumNumberOfStringPairs(words: Array[String]): Int = {\n        val wordSet = scala.collection.mutable.Set[String](words: _*)\n        var count = 0\n\n        for (word <- words) {\n            if (wordSet.contains(word)) {\n                val reverseWord = word.reverse\n                if (wordSet.contains(reverseWord) && word != reverseWord) {\n                    count += 1\n                    wordSet -= word\n                    wordSet -= reverseWord\n                }\n            }\n        }\n\n        count\n    }\n}"
