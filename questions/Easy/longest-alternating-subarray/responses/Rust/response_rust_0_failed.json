    "code": "impl Solution {\n    pub fn alternating_subarray(nums: Vec<i32>) -> i32 {\n        let mut max_length = -1;\n        let mut current_length = 1;\n\n        for i in 1..nums.len() {\n            // Check if the current element alternates with the previous one\n            if (nums[i] - nums[i - 1]).abs() == 1 && (nums[i] - nums[i - 1]) * ((i as i32) % 2 * 2 - 1) > 0 {\n                current_length += 1;\n            } else {\n                // Update max_length if the current alternating subarray ends\n                max_length = max_length.max(current_length);\n                current_length = 1; // Reset for the next potential subarray\n            }\n        }\n\n        // Check at the end in case the last subarray is the longest\n        max_length.max(current_length)\n    }\n}"
