    "code": "public class Solution {\n    public int AlternatingSubarray(int[] nums) {\n        int maxLength = -1;\n        int currentLength = 1;\n\n        for (int i = 1; i < nums.Length; i++) {\n            // Check if current and previous element satisfy alternating condition\n            if ((nums[i] - nums[i - 1] == 1 && currentLength % 2 == 1) ||\n                (nums[i] - nums[i - 1] == -1 && currentLength % 2 == 0)) {\n                currentLength++;\n                maxLength = Math.Max(maxLength, currentLength);\n            } else {\n                // Reset the length if the alternating pattern breaks\n                currentLength = 1;\n            }\n        }\n\n        return maxLength == -1 ? -1 : maxLength;\n    }\n}"
