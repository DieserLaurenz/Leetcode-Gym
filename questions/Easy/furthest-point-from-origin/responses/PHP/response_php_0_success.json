    "code": "class Solution {\n\n    /**\n     * @param String $moves\n     * @return Integer\n     */\n    function furthestDistanceFromOrigin($moves) {\n        // Count the occurrences of 'L', 'R', and '_'\n        $left = substr_count($moves, 'L');\n        $right = substr_count($moves, 'R');\n        $wildcards = substr_count($moves, '_');\n        \n        // Calculate the max possible distance in one direction\n        if ($left > $right) {\n            // If there are more 'L's, we can add the wildcards to 'L's to go further left\n            return $left + $wildcards - $right;\n        } else {\n            // If there are more 'R's or equal, we can add the wildcards to 'R's to go further right\n            return $right + $wildcards - $left;\n        }\n    }\n}"
