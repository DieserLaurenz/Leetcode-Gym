    "code": "impl Solution {\n    pub fn semi_ordered_permutation(nums: Vec<i32>) -> i32 {\n        let n = nums.len() as i32;\n        // Find the index (0-indexed) of 1 and n in nums\n        let pos_1 = nums.iter().position(|&x| x == 1).unwrap() as i32;\n        let pos_n = nums.iter().position(|&x| x == n).unwrap() as i32;\n\n        // Calculate the number of swaps needed\n        // For 1, it's its index (distance from the first position)\n        // For n, it's (nums.len() - 1) - its index (distance from the last position)\n        let swaps_for_1 = pos_1;\n        let swaps_for_n = (n - 1) - pos_n;\n\n        // The total operations is the sum of swaps needed for 1 and n\n        swaps_for_1 + swaps_for_n\n    }\n}"
