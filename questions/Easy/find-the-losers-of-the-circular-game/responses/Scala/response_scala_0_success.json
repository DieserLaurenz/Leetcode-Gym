    "code": "object Solution {\n    def circularGameLosers(n: Int, k: Int): Array[Int] = {\n        val hasBall = scala.collection.mutable.Set[Int]()\n        var currentFriend = 1\n        var step = 1\n\n        while (!hasBall.contains(currentFriend)) {\n            hasBall += currentFriend\n            currentFriend = ((currentFriend - 1 + step * k) % n) + 1\n            step += 1\n        }\n\n        (1 to n).filterNot(hasBall.contains).toArray\n    }\n}"
