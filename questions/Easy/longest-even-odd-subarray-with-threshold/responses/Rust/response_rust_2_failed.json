    "code": "impl Solution {\n    pub fn longest_alternating_subarray(nums: Vec<i32>, threshold: i32) -> i32 {\n        let mut max_length = 0;\n        let mut current_start = None;\n\n        for (i, &num) in nums.iter().enumerate() {\n            // Check if current number exceeds threshold or doesn't alternate\n            if num > threshold || (i > 0 && (num % 2 == nums[i - 1] % 2)) {\n                // Reset current_start if conditions are not met\n                current_start = None;\n            } else {\n                // Set current_start if not already set\n                if current_start.is_none() {\n                    current_start = Some(i);\n                }\n                // Update max_length\n                max_length = max_length.max(i - current_start.unwrap() + 1);\n            }\n        }\n\n        // Ensure the start with an even number\n        if max_length > 0 && nums[current_start.unwrap()] % 2 != 0 {\n            max_length = 0;\n        }\n\n        max_length as i32\n    }\n}"
