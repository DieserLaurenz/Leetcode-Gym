    "code": "class Solution {\n\n    /**\n     * @param Integer[] $nums\n     * @param Integer $threshold\n     * @return Integer\n     */\n    function longestAlternatingSubarray($nums, $threshold) {\n        $maxLength = 0;\n        $n = count($nums);\n        \n        for ($i = 0; $i < $n; $i++) {\n            if ($nums[$i] % 2 == 0 && $nums[$i] <= $threshold) {\n                $currentLength = 1;\n                for ($j = $i + 1; $j < $n; $j++) {\n                    if ($nums[$j] % 2 != $nums[$j - 1] % 2 && $nums[$j] <= $threshold) {\n                        $currentLength++;\n                    } else {\n                        break;\n                    }\n                }\n                $maxLength = max($maxLength, $currentLength);\n            }\n        }\n        \n        return $maxLength;\n    }\n}"
