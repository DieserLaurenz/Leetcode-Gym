    "code": "-spec max_sum(Nums :: [integer()]) -> integer().\nmax_sum(Nums) ->\n    Map = lists:foldl(fun(Num, Acc) ->\n                          MaxDigit = max_digit(Num),\n                          case maps:find(MaxDigit, Acc) of\n                              {ok, Existing} -> maps:update(MaxDigit, [Num | Existing], Acc);\n                              error -> maps:put(MaxDigit, [Num], Acc)\n                          end\n                      end, #{}, Nums),\n    MaxSum = maps:fold(fun(_Key, Values, MaxAcc) ->\n                           case lists:sort(Values) of\n                               [A] -> MaxAcc;\n                               List -> \n                                   [Largest, SecondLargest | _] = lists:reverse(List),\n                                   Sum = Largest + SecondLargest,\n                                   max(MaxAcc, Sum)\n                           end\n                       end, -1, Map),\n    MaxSum.\n\nmax_digit(Number) ->\n    MaxDigit = lists:max(integer_to_list(Number, 10)),\n    list_to_integer([MaxDigit])."
