    "code": "object Solution {\n    def countSymmetricIntegers(low: Int, high: Int): Int = {\n        var count = 0\n\n        for (x <- low to high) {\n            val digits = x.toString.map(_.asDigit)\n            if (digits.length % 2 == 0) {\n                val (firstHalf, secondHalf) = digits.splitAt(digits.length / 2)\n                if (firstHalf.sum == secondHalf.sum) {\n                    count += 1\n                }\n            }\n        }\n\n        count\n    }\n}"
