    "code": "object Solution {\n    def minCost(nums: Array[Int], x: Int): Long = {\n        val n = nums.length\n        var minCost = Long.MaxValue\n\n        // Create a prefix sum array to help with calculating the cost of rotations\n        val prefixSum = Array.fill(n + 1)(0L)\n        for (i <- nums.indices) {\n            prefixSum(i + 1) = prefixSum(i) + nums(i)\n        }\n\n        // Try every possible number of operations from 0 to n - 1\n        for (ops <- 0 until n) {\n            var cost = ops.toLong * x // Cost of performing 'ops' operations\n            // For each chocolate, calculate the cost of buying it after 'ops' rotations\n            for (i <- 0 until n) {\n                val index = (i + ops) % n // The new index of chocolate after 'ops' rotations\n                cost += nums(index)\n            }\n            // Update the minimum cost\n            minCost = math.min(minCost, cost)\n        }\n\n        // Direct purchase without any operations\n        val directPurchaseCost = prefixSum(n)\n        math.min(minCost, directPurchaseCost)\n    }\n}"
