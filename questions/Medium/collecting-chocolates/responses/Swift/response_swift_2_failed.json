    "code": "class Solution {\n    func minCost(_ nums: [Int], _ x: Int) -> Int {\n        let n = nums.count\n        var minCost = Int.max\n        var sum = 0\n        var costWithoutOperation = nums.reduce(0, +) // Cost of buying all chocolates without operations\n        \n        // Calculate the cost with operations for each possible starting point\n        for start in 0..<n {\n            sum = 0 // Reset sum for each starting point\n            \n            // Calculate the total cost of collecting all chocolates starting from 'start' index\n            // and performing operations as needed\n            for i in 0..<n {\n                let index = (start + i) % n // Calculate the new index after i rotations\n                sum += nums[index] // Add the cost of the current chocolate\n                let operationsCost = x * i // Total cost of operations to reach this chocolate\n                \n                // Calculate the total cost up to this point including the cost of operations\n                let totalCost = sum + operationsCost\n                // Update minCost if this total cost is less than the current minCost\n                minCost = min(minCost, totalCost)\n            }\n        }\n        \n        // The minimum cost is the lesser between the cost without operation and the minimum cost found with operations\n        return min(minCost, costWithoutOperation)\n    }\n}"
