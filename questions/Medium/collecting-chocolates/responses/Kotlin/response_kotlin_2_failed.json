    "code": "class Solution {\n    fun minCost(nums: IntArray, x: Int): Long {\n        val n = nums.size\n        var minTotalCost = Long.MAX_VALUE\n\n        // Iterate through all possible starting points\n        for (start in 0 until n) {\n            var currentCost = 0L\n            var operationCost = 0L\n\n            // Calculate the cost to make 'start' the first type we buy\n            for (i in 0 until n) {\n                val currentIndex = (start + i) % n\n                currentCost += nums[currentIndex]\n                minTotalCost = minOf(minTotalCost, currentCost + operationCost)\n                operationCost += x\n            }\n        }\n\n        return minTotalCost\n    }\n}"
