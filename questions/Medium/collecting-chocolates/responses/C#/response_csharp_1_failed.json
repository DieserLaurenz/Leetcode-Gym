    "code": "public class Solution {\n    public long MinCost(int[] nums, int x) {\n        long minCost = long.MaxValue;\n        int n = nums.Length;\n\n        // Try every possible starting point to perform the operation\n        for (int start = 0; start < n; ++start) {\n            long currentCost = 0;\n            long operationCost = 0;\n\n            // Calculate cost for this start point\n            for (int i = 0; i < n; ++i) {\n                int index = (start + i) % n;\n                operationCost = (long)i * x;\n                currentCost += Math.Min(nums[index], operationCost);\n                if (currentCost >= minCost) break; // Early termination if current cost exceeds known minimum\n            }\n\n            minCost = Math.Min(minCost, currentCost);\n        }\n\n        return minCost;\n    }\n}"
