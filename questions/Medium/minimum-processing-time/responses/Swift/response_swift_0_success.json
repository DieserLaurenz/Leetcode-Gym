    "code": "class Solution {\n    func minProcessingTime(_ processorTime: [Int], _ tasks: [Int]) -> Int {\n        let sortedProcessorTime = processorTime.sorted()\n        let sortedTasks = tasks.sorted(by: >)\n        var maxTime = 0\n        var taskIndex = 0\n\n        for time in sortedProcessorTime {\n            var maxTaskTime = 0\n            for _ in 0..<4 {\n                maxTaskTime = max(maxTaskTime, sortedTasks[taskIndex])\n                taskIndex += 1\n            }\n            maxTime = max(maxTime, time + maxTaskTime)\n        }\n        \n        return maxTime\n    }\n}"
