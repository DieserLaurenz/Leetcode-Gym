    "code": "class Solution {\n\n    /**\n     * @param Integer[] $nums\n     * @return Integer\n     */\n    function largestPerimeter($nums) {\n        rsort($nums); // Sort the array in non-increasing order\n        for ($i = 0; $i < count($nums) - 2; ++$i) {\n            // Check if the three numbers can form a triangle\n            if ($nums[$i] < $nums[$i + 1] + $nums[$i + 2]) {\n                // If they can, return their perimeter\n                return $nums[$i] + $nums[$i + 1] + $nums[$i + 2];\n            }\n        }\n        // If no such triplet is found, return -1 indicating no triangle can be formed\n        return -1;\n    }\n}"
