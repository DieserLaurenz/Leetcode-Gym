    "code": "-spec number_of_ways(N :: integer(), X :: integer()) -> integer().\nnumber_of_ways(N, X) ->\n    number_of_ways(N, N, X, maps:new()) rem 1000000007.\n\nnumber_of_ways(0, _, _, _) ->\n    1;\nnumber_of_ways(_, 0, _, _) ->\n    0;\nnumber_of_ways(N, Current, X, Memo) when N < 0 ->\n    0;\nnumber_of_ways(N, Current, X, Memo) ->\n    case maps:find({N, Current}, Memo) of\n        {ok, Result} ->\n            Result;\n        _ ->\n            Power = Current bsl X,\n            WithCurrent = if\n                N - Power >= 0 ->\n                    number_of_ways(N - Power, Current - 1, X, Memo);\n                true ->\n                    0\n            end,\n            WithoutCurrent = number_of_ways(N, Current - 1, X, Memo),\n            Result = (WithCurrent + WithoutCurrent) rem 1000000007,\n            NewMemo = maps:put({N, Current}, Result, Memo),\n            Result\n    end."
