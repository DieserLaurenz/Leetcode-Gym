    "code": "public class Solution {\n    public long MaxScore(int[] nums, int x) {\n        int n = nums.Length;\n        long[] dp = new long[n];\n        dp[0] = nums[0];\n        for (int i = 1; i < n; i++) {\n            dp[i] = dp[i - 1] + nums[i];\n            if (i > 0 && nums[i] % 2 != nums[i - 1] % 2) {\n                dp[i] -= x;\n            }\n        }\n\n        long maxScore = dp[0];\n        for (int i = 1; i < n; i++) {\n            maxScore = Math.Max(maxScore, dp[i]);\n        }\n        return maxScore;\n    }\n}"
