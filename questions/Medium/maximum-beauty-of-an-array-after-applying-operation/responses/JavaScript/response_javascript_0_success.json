    "code": "/**\n * @param {number[]} nums\n * @param {number} k\n * @return {number}\n */\nvar maximumBeauty = function(nums, k) {\n    nums.sort((a, b) => a - b);\n    let maxBeaut = 1, count = 1, l = 0;\n    for (let r = 1; r < nums.length; r++) {\n        while (nums[r] - nums[l] > 2 * k) {\n            l++;\n            count--;\n        }\n        count++;\n        maxBeaut = Math.max(maxBeaut, count);\n    }\n    return maxBeaut;\n};"
