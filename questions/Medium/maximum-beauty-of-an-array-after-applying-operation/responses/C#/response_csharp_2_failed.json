    "code": "public class Solution {\n    public int MaximumBeauty(int[] nums, int k) {\n        Array.Sort(nums);\n        int maxBeauty = 1;\n        long sum = 0;\n        int left = 0;\n        for (int right = 0; right < nums.Length; right++) {\n            sum += nums[right];\n            while ((long)nums[right] * (right - left + 1) - sum > k) {\n                sum -= nums[left++];\n            }\n            maxBeauty = Math.Max(maxBeauty, right - left + 1);\n        }\n        return maxBeauty;\n    }\n}"
