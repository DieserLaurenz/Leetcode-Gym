    "code": "class Solution {\n\n    /**\n     * @param Integer $x\n     * @param Integer $y\n     * @param Integer $z\n     * @return Integer\n     */\n    function longestString($x, $y, $z) {\n        // The strategy is to first append \"AA\" and \"BB\" maximally while inserting \"AB\" to prevent \"AAA\" or \"BBB\"\n        $totalLength = 0;\n\n        // Determine the maximum number of \"AA\" or \"BB\" that can be placed without violating the rules\n        $minXY = min($x, $y);\n        $maxXY = max($x, $y);\n        \n        // If we have \"AB\" to use as a separator, it can be used optimally\n        if ($z > 0) {\n            // Calculate the core string with \"AB\" as a separator\n            $withZ = min($x + $y + $z, $x + $y + 2 * $z - 1); // -1 ensures no \"AB\" at the end if not needed\n            // Calculate without using \"AB\" as a separator, just alternating \"AA\" and \"BB\"\n            $withoutZ = 2 * $minXY + min(2, $maxXY - $minXY);\n            \n            $totalLength = max($withZ, $withoutZ);\n        } else {\n            // Without \"AB\", just alternate \"AA\" and \"BB\"\n            $totalLength = 2 * $minXY + min(2, $maxXY - $minXY);\n        }\n\n        return $totalLength;\n    }\n}"
