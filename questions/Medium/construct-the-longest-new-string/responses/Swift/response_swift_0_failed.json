    "code": "class Solution {\n    func longestString(_ x: Int, _ y: Int, _ z: Int) -> Int {\n        // Sort the counts of \"AA\" and \"BB\" to generalize the approach\n        let sortedCounts = [x, y].sorted()\n        let minAB = sortedCounts[0]\n        let maxAB = sortedCounts[1]\n        let totalAB = z\n\n        // Calculate the maximum possible length considering \"AB\" strings\n        // and ensuring we don't create \"AAA\" or \"BBB\"\n        // We can use up to double the minimum of x and y plus one for each \"AB\" string\n        let baseLength = min(minAB, maxAB - totalAB) * 2 + totalAB * 2\n        let extraAB = max(0, maxAB - (minAB + totalAB))\n\n        // The maximum length considers all usable \"AA\", \"BB\", and \"AB\" strings\n        return baseLength + extraAB * 2\n    }\n}"
