    "code": "class Solution {\n\n    /**\n     * Finds the shortest string that contains all three strings as substrings.\n     * If there are multiple such strings, returns the lexicographically smallest one.\n     *\n     * @param String $a\n     * @param String $b\n     * @param String $c\n     * @return String\n     */\n    function minimumString($a, $b, $c) {\n        $permutations = [\n            [$a, $b, $c],\n            [$a, $c, $b],\n            [$b, $a, $c],\n            [$b, $c, $a],\n            [$c, $a, $b],\n            [$c, $b, $a],\n        ];\n\n        $merge = function ($s1, $s2) {\n            if (strpos($s1, $s2) !== false) return $s1;\n            if (strpos($s2, $s1) !== false) return $s2;\n\n            for ($i = min(strlen($s1), strlen($s2)); $i > 0; $i--) {\n                if (substr($s1, -$i) === substr($s2, 0, $i)) {\n                    return $s1 . substr($s2, $i);\n                }\n                if (substr($s1, 0, $i) === substr($s2, -$i)) {\n                    return $s2 . substr($s1, $i);\n                }\n            }\n\n            return $s1 . $s2;\n        };\n\n        $result = $a . $b . $c; // Initialize with a string definitely long enough\n        foreach ($permutations as $perm) {\n            $temp = $merge($merge($perm[0], $perm[1]), $perm[2]);\n            if (strlen($temp) < strlen($result) || (strlen($temp) == strlen($result) && strcmp($temp, $result) < 0)) {\n                $result = $temp;\n            }\n        }\n\n        return $result;\n    }\n}"
