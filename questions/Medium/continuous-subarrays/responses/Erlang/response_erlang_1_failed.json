    "code": "-spec continuous_subarrays(Nums :: [integer()]) -> integer().\ncontinuous_subarrays(Nums) ->\n    continuous_subarrays(Nums, 1, 0).\n\ncontinuous_subarrays([], _, Count) ->\n    Count;\ncontinuous_subarrays([_H|T] = List, Start, Count) ->\n    End = find_end(List, Start, Start),\n    NewCount = Count + (End - Start + 1),\n    continuous_subarrays(T, 1, NewCount).\n\nfind_end(_, End, Length) when End > Length ->\n    Length;\nfind_end(List, Start, End) ->\n    SubList = lists:sublist(List, Start, End - Start + 1),\n    Min = lists:min(SubList),\n    Max = lists:max(SubList),\n    if\n        Max - Min =< 2 ->\n            find_end(List, Start, End + 1);\n        true ->\n            End - 1\n    end."
