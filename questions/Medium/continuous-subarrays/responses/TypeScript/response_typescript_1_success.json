    "code": "function continuousSubarrays(nums: number[]): number {\n    let total = 0;\n    let start = 0;\n    let minQueue: number[] = [];\n    let maxQueue: number[] = [];\n\n    for (let end = 0; end < nums.length; end++) {\n        while (minQueue.length && nums[minQueue[minQueue.length - 1]] > nums[end]) {\n            minQueue.pop();\n        }\n        while (maxQueue.length && nums[maxQueue[maxQueue.length - 1]] < nums[end]) {\n            maxQueue.pop();\n        }\n\n        minQueue.push(end);\n        maxQueue.push(end);\n\n        while (nums[maxQueue[0]] - nums[minQueue[0]] > 2) {\n            if (minQueue[0] <= start) minQueue.shift();\n            if (maxQueue[0] <= start) maxQueue.shift();\n            start++;\n        }\n\n        total += end - start + 1;\n    }\n\n    return total;\n};"
