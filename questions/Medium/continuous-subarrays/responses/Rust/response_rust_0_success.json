    "code": "impl Solution {\n    pub fn continuous_subarrays(nums: Vec<i32>) -> i64 {\n        let mut total = 0i64;\n        let mut start = 0usize;\n        let mut min_val = nums[0];\n        let mut max_val = nums[0];\n\n        for end in 0..nums.len() {\n            // Update min and max for the current window\n            min_val = min_val.min(nums[end]);\n            max_val = max_val.max(nums[end]);\n\n            // Ensure the continuous condition is met\n            while max_val - min_val > 2 {\n                // Adjust start pointer and min/max values\n                if nums[start] == min_val || nums[start] == max_val {\n                    min_val = *nums[start + 1..=end].iter().min().unwrap();\n                    max_val = *nums[start + 1..=end].iter().max().unwrap();\n                }\n                start += 1;\n            }\n\n            // Update total with the number of subarrays ending at 'end'\n            total += (end - start + 1) as i64;\n        }\n\n        total\n    }\n}"
