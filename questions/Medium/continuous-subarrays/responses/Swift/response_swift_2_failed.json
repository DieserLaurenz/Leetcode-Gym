    "code": "class Solution {\n    func continuousSubarrays(_ nums: [Int]) -> Int {\n        var count = 0\n        var start = 0\n        // The end of the window starts at 0, iterating through the array\n        for end in 0..<nums.count {\n            // Within this loop, adjust the start to maintain the condition\n            // for a continuous subarray\n            var maxVal = nums[start]\n            var minVal = nums[start]\n            for i in start...end {\n                maxVal = max(maxVal, nums[i])\n                minVal = min(minVal, nums[i])\n                if maxVal - minVal > 2 {\n                    start = i + 1\n                    maxVal = nums[start]\n                    minVal = nums[start]\n                }\n            }\n            // For each position of end, the number of continuous subarrays\n            // that can be formed up to this point is added to the count.\n            count += end - start + 1\n        }\n        return count\n    }\n}"
