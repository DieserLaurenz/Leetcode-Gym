    "code": "class Solution {\n    func continuousSubarrays(_ nums: [Int]) -> Int {\n        let n = nums.count\n        var total = 0\n        var left = 0\n\n        for right in 0..<n {\n            // As we move the right pointer, ensure that the subarray [left, right] satisfies the condition\n            // If it does not, move the left pointer to the right until the condition is satisfied again\n            while left < right && abs(nums[right] - nums[left]) > 2 {\n                left += 1\n            }\n            // All subarrays ending at 'right' and starting from 'left' to 'right' are valid\n            // The number of such subarrays is (right - left + 1)\n            total += right - left + 1\n        }\n        \n        return total\n    }\n}"
