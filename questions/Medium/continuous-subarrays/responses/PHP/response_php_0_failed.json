    "code": "class Solution {\n\n    /**\n     * @param Integer[] $nums\n     * @return Integer\n     */\n    function continuousSubarrays($nums) {\n        $count = 0;\n        $left = 0;\n        $n = count($nums);\n        \n        for ($right = 0; $right < $n; $right++) {\n            // Expand the window to the right, ensure it's continuous\n            while ($left < $right && abs($nums[$right] - $nums[$right - 1]) > 2) {\n                $left++;\n            }\n\n            // Check each subarray starting from left to right\n            for ($i = $left; $i <= $right; $i++) {\n                if ($this->isContinuous(array_slice($nums, $i, $right - $i + 1))) {\n                    $count++;\n                } else {\n                    break;\n                }\n            }\n        }\n        \n        return $count;\n    }\n    \n    /**\n     * Helper function to check if the subarray is continuous.\n     */\n    private function isContinuous($subarray) {\n        for ($i = 1; $i < count($subarray); $i++) {\n            if (abs($subarray[$i] - $subarray[$i - 1]) > 2) {\n                return false;\n            }\n        }\n        return true;\n    }\n}"
