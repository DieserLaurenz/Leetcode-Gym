    "code": "class Solution {\n    func minimumCost(_ s: String) -> Int {\n        let n = s.count\n        var flips = [0, 0] // flips[0] for '0' to '1', flips[1] for '1' to '0'\n        var minCost = n // Start with the maximum cost, which is to flip all characters\n        \n        for char in s {\n            // Cost is minimum of flipping all previous '1's to '0's or all previous '0's to '1's and current one\n            minCost = min(minCost, min(flips[0], flips[1]) + 1)\n            flips[char == \"0\" ? 1 : 0] += 1 // Update flips count\n        }\n        \n        // Check last character flip scenario\n        minCost = min(minCost, min(flips[0], flips[1]))\n        \n        return minCost\n    }\n}"
