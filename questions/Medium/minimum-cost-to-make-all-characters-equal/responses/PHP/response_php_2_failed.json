    "code": "class Solution {\n\n    /**\n     * @param String $s\n     * @return Integer\n     */\n    function minimumCost($s) {\n        $costToMakeAllZeros = $costToMakeAllOnes = $cost = 0;\n        $n = strlen($s);\n\n        // Pre-calculate the cost to make all characters '1' from the start and '0' from the end.\n        $ones = array_fill(0, $n + 1, 0);\n        for ($i = 1; $i <= $n; $i++) {\n            $ones[$i] = $ones[$i - 1] + ($s[$i - 1] === '1' ? 1 : 0);\n        }\n\n        $minCost = PHP_INT_MAX;\n        for ($i = 0; $i <= $n; $i++) {\n            // Cost to make all zeros until i and all ones from i to n\n            $costToMakeAllZeros = $i - $ones[$i]; // Flip all '1's to '0's in the left part\n            $costToMakeAllOnes = $ones[$n] - $ones[$i]; // Flip all '0's to '1's in the right part\n            $cost = $costToMakeAllZeros + $costToMakeAllOnes; // Total cost\n            $minCost = min($minCost, $cost);\n        }\n\n        return $minCost;\n    }\n}"
