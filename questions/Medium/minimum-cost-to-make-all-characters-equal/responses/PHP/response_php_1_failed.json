    "code": "class Solution {\n\n    /**\n     * @param String $s\n     * @return Integer\n     */\n    function minimumCost($s) {\n        $n = strlen($s);\n        $cost = 0;\n        $leftCost = 0;\n        $rightCost = 0;\n        $ones = 0;\n\n        // Count the number of ones in the string\n        for ($i = 0; $i < $n; $i++) {\n            if ($s[$i] === '1') {\n                $ones++;\n            }\n        }\n\n        $minCost = $ones; // Cost to invert all ones to zeros\n        for ($i = 0; $i < $n; $i++) {\n            if ($s[$i] === '0') {\n                // If we encounter a zero, increment leftCost\n                $leftCost++;\n            } else {\n                // If we encounter a one, it contributes to rightCost\n                $ones--;\n            }\n            // Minimum cost at this point is either flipping all to the left or all to the right of this point\n            $minCost = min($minCost, $leftCost + $ones);\n        }\n\n        return $minCost;\n    }\n}"
