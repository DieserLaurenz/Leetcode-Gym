    "code": "class Solution {\n    func divideArray(_ nums: [Int], _ k: Int) -> [[Int]] {\n        let sortedNums = nums.sorted()\n        var result = [[Int]]()\n        var temp = [Int]()\n        \n        for num in sortedNums {\n            if temp.count < 3 {\n                temp.append(num)\n            }\n            if temp.count == 3 {\n                if temp.max()! - temp.min()! <= k {\n                    result.append(temp)\n                    temp.removeAll()\n                } else {\n                    return []\n                }\n            }\n        }\n        if !temp.isEmpty {\n            return []\n        }\n        return result\n    }\n}"
