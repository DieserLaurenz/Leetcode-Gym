    "code": "object Solution {\n    def divideArray(nums: Array[Int], k: Int): Array[Array[Int]] = {\n        scala.util.Sorting.quickSort(nums)\n        val result = Array.ofDim[Array[Int]](nums.length / 3)\n        var idx = 0\n        for(i <- result.indices) {\n            if (idx + 2 < nums.length && nums(idx + 2) - nums(idx) <= k) {\n                result(i) = Array(nums(idx), nums(idx + 1), nums(idx + 2))\n                idx += 3\n            } else {\n                return Array[Array[Int]]()\n            }\n        }\n        result\n    }\n}"
