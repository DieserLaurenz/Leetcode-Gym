    "code": "public class Solution {\n    public int[][] DivideArray(int[] nums, int k) {\n        Array.Sort(nums);\n        var result = new List<int[]>();\n        var temp = new List<int>();\n        foreach (var num in nums) {\n            temp.Add(num);\n            if (temp.Count == 3) {\n                if (temp[2] - temp[0] <= k) {\n                    result.Add(temp.ToArray());\n                    temp.Clear();\n                } else {\n                    return new int[0][];\n                }\n            }\n        }\n        return result.ToArray();\n    }\n}"
