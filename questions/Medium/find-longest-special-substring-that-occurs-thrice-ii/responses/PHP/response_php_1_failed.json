    "code": "class Solution {\n\n    /**\n     * @param String $s\n     * @return Integer\n     */\n    function maximumLength($s) {\n        $n = strlen($s);\n        $result = -1;\n        \n        for ($i = 0; $i < $n; ++$i) {\n            for ($j = $i + 1; $j <= $n; ++$j) {\n                $substr = substr($s, $i, $j - $i);\n                if ($substr === str_repeat($s[$i], $j - $i)) {\n                    $count = substr_count($s, $substr);\n                    if ($count >= 3) {\n                        $result = max($result, $j - $i);\n                    }\n                }\n            }\n        }\n\n        return $result;\n    }\n}"
