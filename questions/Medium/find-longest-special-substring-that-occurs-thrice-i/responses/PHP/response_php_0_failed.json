    "code": "class Solution {\n\n    /**\n     * @param String $s\n     * @return Integer\n     */\n    function maximumLength($s) {\n        $maxLen = -1;\n        for ($i = 0; $i < strlen($s); $i++) {\n            for ($j = $i + 1; $j <= strlen($s); $j++) {\n                $substr = substr($s, $i, $j - $i);\n                if (strlen($substr) < $maxLen) continue;\n                if (count(array_unique(str_split($substr))) === 1) {\n                    if (substr_count($s, $substr) >= 3) {\n                        $maxLen = max($maxLen, strlen($substr));\n                    }\n                }\n            }\n        }\n        return $maxLen;\n    }\n}"
