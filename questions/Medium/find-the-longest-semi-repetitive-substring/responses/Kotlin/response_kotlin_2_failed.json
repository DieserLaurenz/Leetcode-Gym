    "code": "class Solution {\n    fun longestSemiRepetitiveSubstring(s: String): Int {\n        var maxLen = 0\n        var i = 0\n        while (i < s.length) {\n            var count = 1\n            var hasPair = false\n            for (j in i + 1 until s.length) {\n                if (s[j] == s[j - 1]) {\n                    if (hasPair || (j > 1 && s[j] == s[j - 2])) {\n                        break\n                    }\n                    hasPair = true\n                }\n                count++\n            }\n            maxLen = maxOf(maxLen, count)\n            i += if (hasPair) 1 else count\n        }\n        return maxLen\n    }\n}"
