    "code": "class Solution {\n    fun longestSemiRepetitiveSubstring(s: String): Int {\n        var maxLength = 0\n        var currentLength = 0\n        var pairSeen = false\n\n        for (i in s.indices) {\n            if (i > 0 && s[i] == s[i - 1]) {\n                if (pairSeen) {\n                    maxLength = maxOf(maxLength, currentLength)\n                    currentLength -= 1\n                    pairSeen = false\n                } else {\n                    pairSeen = true\n                }\n            } else {\n                if (i > 1 && s[i] == s[i - 2]) {\n                    maxLength = maxOf(maxLength, currentLength)\n                    currentLength = 2\n                    pairSeen = true\n                } else {\n                    pairSeen = false\n                }\n            }\n            currentLength += 1\n        }\n        return maxOf(maxLength, currentLength)\n    }\n}"
