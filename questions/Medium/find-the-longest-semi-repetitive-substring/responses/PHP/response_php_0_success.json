    "code": "class Solution {\n\n    /**\n     * @param String $s\n     * @return Integer\n     */\n    function longestSemiRepetitiveSubstring($s) {\n        $maxLen = 0;\n        $len = strlen($s);\n        for ($i = 0; $i < $len; $i++) {\n            $repeatCount = 0;\n            for ($j = $i; $j < $len; $j++) {\n                if ($j > $i && $s[$j] == $s[$j - 1]) {\n                    $repeatCount++;\n                }\n                if ($repeatCount > 1) break;\n                $maxLen = max($maxLen, $j - $i + 1);\n            }\n        }\n        return $maxLen;\n    }\n}"
