    "code": "public class Solution {\n    public int LongestSemiRepetitiveSubstring(string s) {\n        int maxLength = 1;\n        for (int i = 0; i < s.Length; i++) {\n            for (int j = i + 1; j < s.Length; j++) {\n                int count = 0;\n                bool valid = true;\n                for (int k = i; k < j; k++) {\n                    if (s[k] == s[k + 1]) {\n                        count++;\n                        if (count > 1) {\n                            valid = false;\n                            break;\n                        }\n                    } else {\n                        count = 0;\n                    }\n                }\n                if (valid) {\n                    maxLength = Math.Max(maxLength, j - i + 1);\n                }\n            }\n        }\n        return maxLength;\n    }\n}"
