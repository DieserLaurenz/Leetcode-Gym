    "code": "class Solution {\n    func longestSemiRepetitiveSubstring(_ s: String) -> Int {\n        var maxLength = 0\n        var left = 0\n        var right = 0\n        let chars = Array(s)\n        var lastChar: Character? = nil\n        var lastCharCount = 0\n        \n        while right < chars.count {\n            if chars[right] == lastChar {\n                lastCharCount += 1\n            } else {\n                lastChar = chars[right]\n                lastCharCount = 1\n            }\n            \n            if lastCharCount > 2 {\n                left = right - 1\n                lastCharCount = 2\n            }\n            \n            maxLength = max(maxLength, right - left + 1)\n            right += 1\n        }\n        \n        return maxLength\n    }\n}"
