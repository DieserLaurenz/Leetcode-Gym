    "code": "class Solution {\n    func longestSemiRepetitiveSubstring(_ s: String) -> Int {\n        var maxLen = 0, lastRepeatIndex = -1, start = 0\n        let chars = Array(s)\n        for i in 1..<chars.count {\n            if chars[i] == chars[i - 1] {\n                if lastRepeatIndex != -1 && lastRepeatIndex + 1 < i {\n                    maxLen = max(maxLen, i - start)\n                    start = lastRepeatIndex + 1\n                }\n                lastRepeatIndex = i\n            }\n        }\n        return max(maxLen, chars.count - start)\n    }\n}"
