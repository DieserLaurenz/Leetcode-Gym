    "code": "class Solution {\n\n    /**\n     * @param Integer[] $nums\n     * @param String $s\n     * @param Integer $d\n     * @return Integer\n     */\n    function sumDistance($nums, $s, $d) {\n        $mod = 1000000007;\n        $n = count($nums);\n        $pairs = [];\n        for ($i = 0; $i < $n; $i++) {\n            for ($j = $i + 1; $j < $n; $j++) {\n                $distance = abs($nums[$i] - $nums[$j]);\n                $direction = ($s[$i] === $s[$j]) ? $distance : max(0, $distance - 2 * $d);\n                $pairs[] = $direction;\n            }\n        }\n        $sum = 0;\n        foreach ($pairs as $pair) {\n            $sum = ($sum + $pair) % $mod;\n        }\n        return $sum;\n    }\n}"
