    "code": "object Solution {\n    def sumDistance(nums: Array[Int], s: String, d: Int): Int = {\n        val mod = 1000000007L\n        val n = nums.length\n        val positions = Array.ofDim[Long](n)\n        for (i <- nums.indices) {\n            positions(i) = nums(i).toLong + (if (s(i) == 'R') d else -d)\n        }\n        scala.util.Sorting.quickSort(positions)\n        var sum: Long = 0\n        for (i <- positions.indices) {\n            sum = (sum + positions(i) * i - positions(i) * (n - i - 1)) % mod\n        }\n        ((sum + mod) % mod).toInt\n    }\n}"
