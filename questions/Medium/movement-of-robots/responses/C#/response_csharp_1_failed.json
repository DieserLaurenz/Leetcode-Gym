    "code": "public class Solution {\n    public int SumDistance(int[] nums, string s, int d) {\n        int mod = 1000000007;\n        long sum = 0;\n        for (int i = 0; i < nums.Length; i++) {\n            for (int j = i + 1; j < nums.Length; j++) {\n                long initialDistance = Math.Abs(nums[i] - nums[j]);\n                long distanceAfterD = initialDistance;\n                if (s[i] == s[j]) {\n                    distanceAfterD += 2L * d;\n                } else {\n                    if (initialDistance <= 2L * d) {\n                        distanceAfterD = 2L * d - initialDistance;\n                    }\n                }\n                sum = (sum + distanceAfterD) % mod;\n            }\n        }\n        return (int)sum;\n    }\n}"
