    "code": "object Solution {\n    def incremovableSubarrayCount(nums: Array[Int]): Long = {\n        val n = nums.length\n        val left = Array.fill(n)(0)\n        val right = Array.fill(n)(0)\n        var ans = 0L\n\n        for (i <- 1 until n) {\n            if (nums(i) > nums(i - 1)) left(i) = left(i - 1) + 1\n        }\n\n        for (i <- n - 2 to 0 by -1) {\n            if (nums(i) < nums(i + 1)) right(i) = right(i + 1) + 1\n        }\n\n        for (i <- nums.indices) {\n            ans += (left(i) + 1) * (right(i) + 1) - 1\n        }\n\n        ans\n    }\n}"
