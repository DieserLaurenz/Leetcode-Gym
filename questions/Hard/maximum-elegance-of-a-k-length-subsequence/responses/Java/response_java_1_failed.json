    "code": "class Solution {\n    public long findMaximumElegance(int[][] items, int k) {\n        Arrays.sort(items, (a, b) -> b[0] - a[0]);\n        Map<Integer, Integer> categoryCount = new HashMap<>();\n        long totalProfit = 0;\n        int distinctCategories = 0;\n        \n        for (int i = 0; i < k; i++) {\n            totalProfit += items[i][0];\n            if (categoryCount.putIfAbsent(items[i][1], 1) == null) {\n                distinctCategories++;\n            }\n        }\n        \n        long maxElegance = totalProfit + (long) distinctCategories * distinctCategories;\n        \n        PriorityQueue<Integer> heap = new PriorityQueue<>();\n        for (int i = k; i < items.length; i++) {\n            if (categoryCount.getOrDefault(items[i][1], 0) == 0) {\n                heap.offer(items[i][0]);\n            }\n        }\n        \n        for (int i = 0; i < k; i++) {\n            if (categoryCount.get(items[i][1]) > 1) {\n                if (!heap.isEmpty() && heap.peek() > items[i][0]) {\n                    totalProfit += heap.poll() - items[i][0];\n                    maxElegance = Math.max(maxElegance, totalProfit + (long) distinctCategories * distinctCategories);\n                }\n            } else {\n                categoryCount.put(items[i][1], categoryCount.get(items[i][1]) - 1);\n            }\n        }\n        \n        return maxElegance;\n    }\n}"
