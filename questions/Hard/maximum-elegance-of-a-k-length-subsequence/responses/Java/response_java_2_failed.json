    "code": "class Solution {\n    public long findMaximumElegance(int[][] items, int k) {\n        Arrays.sort(items, (a, b) -> b[0] - a[0]); // Sort items based on profit in descending order\n        Map<Integer, Integer> categoryFrequency = new HashMap<>(); // Track frequency of each category in the top k items\n        long totalProfit = 0; // Sum of profits for top k items\n        int distinctCategories = 0; // Count of distinct categories in top k items\n        for (int i = 0; i < k; i++) {\n            totalProfit += items[i][0];\n            if (categoryFrequency.putIfAbsent(items[i][1], 1) == null) {\n                distinctCategories++;\n            } else {\n                categoryFrequency.put(items[i][1], categoryFrequency.get(items[i][1]) + 1);\n            }\n        }\n\n        long maxElegance = totalProfit + (long) distinctCategories * distinctCategories;\n\n        // Attempt to replace items with lower profit but unique categories to increase elegance\n        for (int i = k; i < items.length && distinctCategories < k; i++) {\n            if (!categoryFrequency.containsKey(items[i][1])) {\n                // If the item belongs to a new category, include it to increase the number of distinct categories\n                distinctCategories++;\n                totalProfit += items[i][0];\n                maxElegance = Math.max(maxElegance, totalProfit + (long) distinctCategories * distinctCategories);\n                categoryFrequency.put(items[i][1], 1);\n            }\n        }\n\n        return maxElegance;\n    }\n}"
