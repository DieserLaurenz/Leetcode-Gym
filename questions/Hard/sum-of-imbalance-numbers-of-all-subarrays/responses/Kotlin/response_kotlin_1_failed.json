    "code": "class Solution {\n    fun sumImbalanceNumbers(nums: IntArray): Int {\n        var sum = 0\n        val n = nums.size\n        nums.forEachIndexed { index, _ ->\n            val left = IntArray(n)\n            val right = IntArray(n)\n            var top = -1\n            for (i in index until n) {\n                while (top != -1 && nums[i] > nums[right[top]]) {\n                    val idx = right[top--]\n                    val leftBound = if (top == -1) index - 1 else right[top]\n                    sum += (i - idx) * (idx - leftBound)\n                }\n                right[++top] = i\n            }\n            while (top != -1) {\n                val idx = right[top--]\n                val leftBound = if (top == -1) index - 1 else right[top]\n                sum += (n - idx) * (idx - leftBound)\n            }\n        }\n        return sum\n    }\n}"
