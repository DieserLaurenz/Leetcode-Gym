    "code": "class Solution {\n\n    /**\n     * @param Integer[] $nums\n     * @return Integer\n     */\n    function sumImbalanceNumbers($nums) {\n        $n = count($nums);\n        $result = 0;\n\n        for ($i = 0; $i < $n; ++$i) {\n            $min = $nums[$i];\n            $max = $nums[$i];\n            for ($j = $i; $j < $n; ++$j) {\n                $min = min($min, $nums[$j]);\n                $max = max($max, $nums[$j]);\n                $gapCount = 0;\n                $countValues = array_count_values(array_slice($nums, $i, $j - $i + 1));\n                for ($k = $min; $k <= $max; ++$k) {\n                    if (!isset($countValues[$k])) {\n                        $gapCount++;\n                    }\n                }\n                $result += $gapCount;\n            }\n        }\n\n        return $result;\n    }\n}"
