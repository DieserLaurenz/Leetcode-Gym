    "code": "public class Solution {\n    public int PaintWalls(int[] cost, int[] time) {\n        int n = cost.Length;\n        int[][] walls = new int[n][];\n        for (int i = 0; i < n; i++) {\n            walls[i] = new int[] { cost[i], time[i] };\n        }\n        Array.Sort(walls, (a, b) => a[1].CompareTo(b[1]));\n        \n        int totalTime = 0, freePainterTime = 0, totalCost = 0;\n        foreach (var wall in walls) {\n            if (freePainterTime < wall[1]) {\n                totalCost += wall[0];\n                totalTime += wall[1];\n                freePainterTime += wall[1];\n            } else {\n                totalTime = Math.Max(totalTime, freePainterTime + 1);\n                freePainterTime++;\n            }\n        }\n        return totalCost;\n    }\n}"
